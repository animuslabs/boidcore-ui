{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "noUncheckedIndexedAccess": true,
    "strictNullChecks": true,
    "useDefineForClassFields": false,
    "target": "ES2020",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "types": [
      "vite/client",
      "node"
    ],
    "baseUrl": "."
  },
  "vueCompilerOptions": {
    "plugins": [
      "@volar/vue-language-plugin-pug",
      "@vue/language-plugin-pug"
    ]
  }
}
